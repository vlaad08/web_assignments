  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p tsconfig.json",
    "start": "node .build/src/index.js",
    "format": "npx prettier --write .",
    "migrate:make": "knex migrate:make",
